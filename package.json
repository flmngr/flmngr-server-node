{
  "name": "@flmngr/flmngr-server-node",
  "description": "Node server-side implementation of Flmngr file manager",
  "version": "1.5.2",
  "private": false,
  "author": {
    "name": "EdSDK",
    "url": "https://edsdk.com/"
  },
  "homepage": "https://flmngr.com",
  "repository": "https://github.com/flmngr/flmngr-server-node",
  "license": "LGPL v3",
  "scripts": {
    "clean": "rm -Rf dist",
    "build": "npm run clean && tsc -b ./tsconfig.json ./tsconfig.esm.json"
  },
  "type": "commonjs",
  "dependencies": {
    "blurhash": "^2.0.5",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "express": "^4",
    "body-parser": "^1",
    "connect-busboy": "^1",
    "sharp": "^0",
    "cors": "^2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/busboy": "^0.2.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^10.17.60",
    "@types/sharp": "^0.21.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "file-manager",
    "fileManager",
    "file",
    "image",
    "manager",
    "server",
    "node"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "engines" : {
    "node" : ">=14.15.0"
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js"
}
